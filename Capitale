package projetgit;

public class Capitale extends Ville {
	private String monument;

        public Capitale(String nome, int habt, String monu){
		super(nome, habt);
		monument=monu;
	}

        String getmonument() {
		return monument;
	}
	void setmonument(String monu) {
		monument=monu;
	}

       void description() {
		super.description();
		System.out.println("et est aussi la capitale avec "+this.getmonument()+" comme monument");
		System.out.println("");
	} 
}
