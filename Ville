package projetgit;

public class Ville {
	private String nom;
	private int hbt;

        public Ville(String nome, int habt) {
		nom=nome;
		hbt=habt;
	}

        void Setnom(String nome) {
		nom=nome;
	}
	void Sethbt(int habt) {
		hbt=habt;
	}
	
	String getnom() {
		return nom;
	}
	int gethbt() {
		return hbt;
	}

        void description() {
		System.out.println("la ville de "+this.getnom()+" a "+this.gethbt()+" habitants");
		//System.out.println("");
	}
}
